<?xml?>
<pal script = "amq_pal_gen">
    This script demonstrates failover.
    <set name = "username" cmdline = "U" value = "guest" />
    <session server = "localhost 127.0.0.1" failover = "5000">
        <queue_declare queue = "failover" />
        <repeat>
            <wait timeout = "1000" />
            <basic_get queue = "failover" />
            <basic_arrived>
                <echo>Message arrived</echo>
            </basic_arrived>
        </repeat>
    </session>
</pal>
