if test -f $1.icl; then
    for a in $1*.icl; do gsl -q $a; done
    c    $1*.c | head -10
    c -r libamq $1*.o
    for a in $1*_test.c; do c -l $a; done

elif test -f $1.smt; then
    gsl -q $1.smt
    c    $1*.c | head -10
    c -r libamq $1*.o

elif test -f $1.xml; then
    gsl -q $1.xml
    c    $1*.c | head -10
    c -r libamq $1*.o

elif test -f $1.c; then
    c    $1*.c | head -10
    c -r libamq $1*.o
fi
