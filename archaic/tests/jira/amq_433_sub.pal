<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = ""
    >
    <set name = "qname"   value = "def"     cmdline = "Z" />
    <session>
        <exchange_declare exchange = "hexchange" type = "headers" />
        <queue_declare queue = "$qname" shared = "1" />
        <queue_bind queue = "$qname" exchange = "hexchange" />
        <basic_consume queue = "$qname" />
        <repeat>
            <wait />
            <basic_arrived>
                <echo>Message '$message_id' arrived</echo>
            </basic_arrived>
        </repeat>
    </session>
    
</pal>


