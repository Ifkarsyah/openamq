<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = "GRM-like publisher"
    >
    
    <session>
        <set name = "a" value = "verb1" cmdline = "V" />
        <set name = "b" value = "test" cmdline = "T" />
        <exchange_declare exchange = "pubsub" type = "headers" />
        <repeat times = "5000" counter = "count">
            <basic_content size = "4096" message_id = "id-$count">
                <headers>
                    <field name = "Verb" value = "$a" />
                    <field name = "Text" value = "$b" />
                </headers>
            </basic_content>
            <basic_publish exchange = "pubsub" routing_key = "pubsub" />
        </repeat>
    </session>
</pal>
