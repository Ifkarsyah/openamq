<?xml?>
<pal script = "amq_pal_gen" >
    <session>
        <exchange_declare exchange = "d_1" type = "direct" />
        <queue_declare queue = "dq_1" auto_delete = "1" />
        <queue_bind queue = "dq_1" exchange = "d_1" routing_key = "dr_1" />
        <queue_bind queue = "dq_1" exchange = "d_1" routing_key = "dr_2" />
        
        <basic_content message_id = "id-1" />
        <basic_consume queue = "dq_1" />
        <basic_publish exchange = "d_1" routing_key = "dr_1" />
        <wait timeout = "1000"/>
        <basic_arrived>
            <echo>msg</echo>
        </basic_arrived>
        <empty>
            <echo>Message did not come back, this is bad!</echo>
        </empty>
        <basic_cancel consumer_tag = "$consumer_tag" />
    </session>
</pal>


