<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = "the server MUST handle basic cancel requests"
    >
    This test creates a queue (bound by default) attaches a consumer, and then
    cancels the consume.

    <include filename = "amq_test_base.pal" />
    <session>
        <queue_declare />
        <basic_consume queue = "$queue" />
        <echo>$consumer_tag works in echo, but not as a value it seems</echo>
        <set name = "ct" value = "$consumer_tag" />
        <basic_cancel queue = "$queue" consumer = "$ct" />
    </session>
    
</pal>


