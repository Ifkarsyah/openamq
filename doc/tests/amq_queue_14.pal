<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = "Bind to an unknown exchange"
    >
    This test sets up a session, declares a queue and tries to bind it to 
    an unknown exchange.
    
    <session error_handling = "recover">
        <queue_declare queue = "myqueue" />
        <queue_bind queue = "myqueue" exchange = "unknown" />
    </session>
</pal>


