<?xml?>
<pal
    script = "amq_pal_gen"
    desc   = "the server MUST provide the client with a consumer tag"
    >
    This test checks if the server provides the client with a consumer tag.
    <include filename = "amq_test_base.pal" />
    <session>
        <queue_declare />
        <basic_consume queue = "$queue" exclusive = "1" />
        <assert name = "consumer_tag" />
    </session>
    
</pal>


