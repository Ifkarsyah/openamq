<?xml?>
<pal
    name = "amq_queue_bind-many"
    script = "amq_pal_gen"
    target = "stdc"
    >
    
    <repeat times = "100000000" counter = "index">
        <session scope="default">
            <exchange_declare exchange="tmp" class="fanout" />
            <queue_declare queue="queue_$index" scope="default" />
            <queue_bind queue="queue_$index" scope="default" exchange="tmp" />
        </session>
    </repeat>
</pal>
