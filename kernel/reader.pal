<?xml?>
<pal script = "amq_pal_gen">
    <session>
        <repeat>
            <repeat counter = "qid" times = "10">
                <queue_declare queue = "queue$qid" auto_delete = "1" />
                <basic_consume queue = "queue$qid" />
                <basic_cancel consumer_tag = "$consumer_tag" />
            </repeat>
            <wait timeout = "100" />
        </repeat>
    </session>
</pal>

