<?xml?>
<pal script = "amq_pal_gen">
    <set name = "ratio" value = "16"   cmdline = "R" />
    <set name = "batch" value = "5000" cmdline = "B" />
    <session>
        <repeat times = "$ratio">
            <queue_declare />
            <queue_bind queue = "$queue" exchange = "amq.fanout" />
        </repeat>
        <basic_content size = "250" />
        <repeat times = "$batch">
            <basic_publish exchange = "amq.fanout" />
        </repeat>
    </session>
</pal>
