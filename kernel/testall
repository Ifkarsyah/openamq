#  Runs all class tests

function test_one {
    echo -n "Testing $1..."
    $1_test
    test $? -ne 0 && exit
    echo " OK"
}

test_one amq_vhost
test_one amq_connection
test_one amq_channel
test_one amq_handle
test_one amq_message
test_one amq_smessage
test_one amq_bucket
test_one amq_field_list
test_one amq_browser_array

rm -r .spool

